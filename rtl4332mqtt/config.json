{
    "name": "RTL433 To MQTT",
    "version": "0.0.1",
    "slug": "rtl4332mqtt",
    "description": "An RTL433 To MQTT Bridge",
    "arch": ["amd64", "i386", "armhf", "armv7", "aarch64"],
    "url": "https://github.com/Zipsnet/hassio-addons/tree/master/rtl4332mqtt",
    "startup": "services",
    "boot": "auto",
    "map": [
	"share"
    ],
    "usb": true,
    "options":
    {
    "mqtt_host": "192.168.1.100",
    "mqtt_port": 1883,
    "mqtt_user": "user",
    "mqtt_password": "pass",
    "mqtt_topic": "rtl_433",
    "mqtt_retain" : "true",
    "frequency" : "-f 915M",
    "protocol" : "142",
    "whitelist_enable": "false",
    "whitelist": "",
    "units": "native",
    "discovery_prefix": "homeassistant",
    "discovery_interval": 600,
    "debug": "false"
  },
    "schema":
    {
    "mqtt_host": "str",
    "mqtt_port": "int",
    "mqtt_user": "str",
    "mqtt_password": "str",
    "mqtt_topic": "str",
    "mqtt_retain" : "str",
    "frequency" : "str",
    "protocol": "str",
    "whitelist_enable": "bool",
    "whitelist": "str",
    "units": "str",
    "discovery_prefix": "str",
    "discovery_interval": "int",
    "debug": "bool"
    },
    "image": "smarthouseint/addon-rtl4332mqtt"
}

{
    "name": "RTL433MQTT to Home Assistant",
    "version": "0.0.1",
    "slug": "rtl4332mqtt",
    "description": "An RTL433 To MQTT Bridge",
    "arch": ["amd64", "i386", "armhf", "armv7", "aarch64"],
    "url": "https://github.com/Zipsnet/hassio-addons/tree/main/rtl4332mqtt",

    "startup": "services",
    "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
    "usb": true,
    "options":
    {
    "mqtt_host": "192.168.1.100",
    "mqtt_port": 1883,
    "mqtt_user": "user",
    "mqtt_password": "pass",
    "mqtt_retain" : "true
    "mqtt_topic": "homeassistant/sensor/rtl_433/P142/+/events",
    "discovery_prefix": "homeassistant",
    "discovery_interval": 600,
    "debug": "false"
  },
    "schema":
    {
    "mqtt_host": "str?",
    "mqtt_port": "int?",
    "mqtt_user": "str?",
    "mqtt_password": "str?",
    "mqtt_retain": "bool",
    "mqtt_topic": "str",
    "discovery_prefix": "str",
    "discovery_interval": "int",
    "debug" "bool"
    },
}
